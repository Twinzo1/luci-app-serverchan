#!/bin/sh
touch /etc/config/serverchan

uci -q batch <<-EOF >/dev/null
	delete ucitrack.@serverchan[-1]
	add ucitrack serverchan
	set ucitrack.@serverchan[-1].init=serverchan
	commit ucitrack
EOF

chmod 777 /etc/init.d/serverchan
chmod 777 /usr/bin/serverchan/serverchan
#chmod -R 777 /usr/bin/serverchan/wrtbwmon
rm -f /tmp/luci-indexcache
exit 0
